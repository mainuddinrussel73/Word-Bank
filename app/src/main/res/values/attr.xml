<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BlurLayout">
        <attr name="downscaleFactor" format="float" />
        <attr name="blurRadius" format="integer" />
        <attr name="fps" format="integer" />
    </declare-styleable>

    <declare-styleable name="ProductionProgressItemView">
        <attr name="progressTitle" format="string"/>
        <attr name="progressV" format="string"/>
        <attr name="progressPercent" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SpreadView">
        <attr name="spread_center_color" format="color" />
        <attr name="spread_radius" format="integer" />
        <attr name="spread_spread_color" format="color" />
        <attr name="spread_distance" format="integer" />
        <attr name="spread_max_radius" format="integer" />
        <attr name="spread_delay_milliseconds" format="integer" />
    </declare-styleable>
</resources>